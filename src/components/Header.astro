---
import NavHeaderWrapper from "./layout/NavHeaderWrapper";
---

  <header
    class="sticky top-0 z-10 w-full border-b border-border/40 bg-background/95 backdrop-blur supports-[backdrop-filter]:bg-background/60"
  >
    <div class="container flex h-14 max-w-screen-2xl items-center">
      <div class="mr-4 flex">
        <a class="mr-6 flex items-center space-x-2" href="/">
          <span class="font-bold">CacheTest PWA</span>
        </a>
      </div>
      <div
        class="flex flex-1 items-center justify-between space-x-2 md:justify-end"
      >
        <nav class="flex items-center gap-4">
          <NavHeaderWrapper client:load />
        </nav>
      </div>
    </div>
  </header>
